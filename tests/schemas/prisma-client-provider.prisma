generator client {
  provider = "prisma-client"
  output   = "../generated/providers/prisma-client"
}

generator class_validator {
  provider = "node ./lib/generator.js"
  output   = "../generated/providers/prisma-client-class-validator"
}

datasource db {
  provider = "sqlite"
  url      = "file:./provider-client.db"
}

model User {
  id    Int    @id @default(autoincrement())
  email String @unique
}
